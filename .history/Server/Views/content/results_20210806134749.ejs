<div class="container">
<br>
<h1 class="headerText"><%=resSet[0].title%></h1>
<hr>
<%if(resSet[0].res[0]!=null){%>
<%
let responseCount=0;
for(let i=0;i<resSet[0].res.length;i++){
    responseCount=responseCount+resSet[0].res[i].total;
}
%>
<div class="row">
<h5 class="headerText" style="text-align:center;">These statistics are based on <%=responseCount%> results</h5>
<div float-md-left>
<button class="btn btn-success"  onclick="print()">Print Results</button> </div>
</div>
<!--divs for charts-->
<%for(let i=0;i<resSet.length;i++){%>
<div id="resChart<%=i%>"></div>
<%}%>
<%}else{%>
<h4>No results to show</h4>
<%}%>
</div>
<!--Scripts to display charts-->
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script>const chartData=<%-JSON.stringify(resSet)%>;</script>
<script src="/Scripts/charts.js"></script>
