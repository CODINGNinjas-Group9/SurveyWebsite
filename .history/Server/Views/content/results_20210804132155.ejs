<!DOCTYPE html>
<html lang="en-US">
<body>

<h2><%=title%></h2>
<%for(let i=0;i<resSet.length;i++){%>
<div id="responseChart"+i></div>
<%}%>
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

<script type="text/javascript">
// Load google charts
google.charts.load('current', {'packages':['corechart']});
google.charts.setOnLoadCallback(drawChart);

// Draw the chart and set the chart values
function drawChart() {
    for(int i=0;i<resSet.length;i++){
  var data = google.visualization.arrayToDataTable([
  ['Question', 'Response'],
  [<%=resSet[i].res[0].resText, resSet[i].res[0].total%>],
  [<%=resSet[i].res[1].resText, resSet[i].res[1].total%>],
  [<%=resSet[i].res[2].resText, resSet[i].res[2].total%>],
  [<%=resSet[i].res[3].resText, resSet[i].res[3].total%>],
  [<%=resSet[i].res[4].resText, resSet[i].res[4].total%>],
]);

  // Optional; add a title and set the width and height of the chart
  var options = {'title':resSet[i].question, 'width':550, 'height':400};

  // Display the chart inside the <div> element with id="piechart"
  var chart = new google.visualization.PieChart(document.getElementById('responseChart'+i));
  chart.draw(data, options);
}
}
</script>

</body>
</html>
